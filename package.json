{
  "name": "eGOn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node ./buildScript",
    "lint": "vue-cli-service lint",
    "install-requirements": "pip install -r server/requirements.txt",
    "start": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "python3 server/server.py || py -3 server/server.py || python server/server.py",
    "start-frontend": "vue-cli-service serve --open"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "fs-extra": "8.1.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.3",
    "@vue/cli-plugin-eslint": "^4.0.3",
    "@vue/cli-service": "^4.0.3",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^1.1.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:3001"
}
